{
    "name": "segic/yii2-extended-gii",
    "description": "The Gii extension for the Yii framework with boilerplate template support",
    "keywords": ["yii2", "gii", "boilerplate", "code generator"],
    "type": "yii2-extension",
    "authors": [
        {
            "name": "Gilles Grandguillaume",
            "email": "gilles.grandguillaume@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": ">=2.0.5",
        "yiisoft/yii2-bootstrap4": "~2.0.8",
        "phpspec/php-diff": ">=1.1.0",
        "bower-asset/typeahead.js": "0.10.* | ~0.11.0"
    },
    "autoload": {
        "psr-4": {
            "aayaresko\\gii\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
